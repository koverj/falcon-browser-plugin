{
  "name": "koverj",
  "description": "",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "*://*/locators",
    "storage",
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "jquery-3.3.1.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "coverage for locators",
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
